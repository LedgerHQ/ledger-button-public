    "@swc/helpers": "~0.5.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.1.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.0.0",
    "@types/react": "19.0.0",
